{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "globals": {
      "__dirname": true,
      "window": true,
      "document": true,
      "localStorage": true,
      "CONFIG": true,
      "process": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "eslint-config-prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": "2018"
    },
    "plugins": ["react", "react-hooks", "eslint-plugin-cypress"],
    "settings": {
      "react": {
        "version": "16.8.6"
      }
    },
    "rules": {
      "react/prop-types": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "off"
    }
}
