{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "globals": {
      "__dirname": true,
      "window": true,
      "document": true,
      "localStorage": true,
      "CONFIG": true,
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "eslint-config-prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": "2018"
    },
    "plugins": ["react", "eslint-plugin-cypress"],
    "settings": {
      "react": {
        "version": "16.8.6"
      }
    },
    "rules": {}
}
