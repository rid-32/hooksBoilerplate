{
  "name": "gault-millau",
  "version": "1.0.0",
  "description": "Gault&Millau frontend application",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --colors --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "serve": "serve -s ./build -l 3000",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "cy:run": "cypress run",
    "cy:run:update": "yarn cy:run -- --env updateSnapshots=true",
    "cy:open": "cypress open",
    "cy:open:update": "yarn cy:open -- --env updateSnapshots=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "osinit.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/cypress": "^4.0.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "config": "^3.2.2",
    "config-webpack": "^1.0.4",
    "css-loader": "^3.1.0",
    "cypress": "^3.4.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "serve": "^11.1.0",
    "style-loader": "^0.23.1",
    "test-data-bot": "^0.8.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
